<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>airbrake.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>lever</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>lever</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>lever</object-name>
    <property>surface-positions/speedbrake-pos-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>  45 </dep></entry>
      <entry><ind> 0.5 </ind><dep>   0 </dep></entry>
      <entry><ind> 1.0 </ind><dep> -45 </dep></entry>
    </interpolation>
    <center>
      <x-m> -0.01 </x-m>
      <y-m>  0.00 </y-m>
      <z-m>  0.00 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
