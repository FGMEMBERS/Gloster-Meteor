<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>engine-ger.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needlefuel</object-name>
    <object-name>needleoil</object-name>
    <object-name>visses</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needlefuel</object-name>
    <object-name>needleoil</object-name>
    <emission>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
      <red>   0.75 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needlefuel</object-name>
    <property>engines/engine[0]/egt-degf</property>
    <min>   0 </min>
    <max> 200 </max>
    <interpolation>
      <entry><ind>   32 </ind><dep>   0 </dep></entry>
      <entry><ind>  392 </ind><dep>  45 </dep></entry>
      <entry><ind>  752 </ind><dep>  90 </dep></entry>
      <entry><ind> 1112 </ind><dep> 135 </dep></entry>
      <entry><ind> 1472 </ind><dep> 180 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.004 </y-m>
      <z-m>  0.000 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needleoil</object-name>
    <property>engines/engine[1]/egt-degf</property>
    <min>  0 </min>
    <max> 25 </max>
    <interpolation>
     <entry><ind>   32 </ind><dep>   0 </dep></entry>
     <entry><ind>  392 </ind><dep>  45 </dep></entry>
     <entry><ind>  752 </ind><dep>  90 </dep></entry>
     <entry><ind> 1112 </ind><dep> 135 </dep></entry>
     <entry><ind> 1472 </ind><dep> 180 </dep></entry>
    </interpolation>
    <center>
     <x-m> 0.000 </x-m>
     <y-m> 0.004 </y-m>
     <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
