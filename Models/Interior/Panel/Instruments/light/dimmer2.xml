<?xml version="1.0" encoding="UTF-8"?>

<PropertyList> 

  <path>dimmer2.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>Knob</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Knob</object-name>
    <emission>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
      <red>   0.75 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>Knob</object-name>
    <property>controls/lighting/instrument-norm</property>
    <interpolation> 
      <entry><ind> 0 </ind><dep>  160 </dep></entry>
      <entry><ind> 1 </ind><dep> -160 </dep></entry>
    </interpolation>
    <axis> 
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- *** LoD for the whole model *** -->
  <animation> 
    <type>range</type>
    <min-m>0</min-m>
    <max-m>5</max-m>
  </animation>

</PropertyList>

