<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

	<path>mach.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
      <red>   0.75 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>velocities/mach</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>   0.0 </dep></entry>
      <entry><ind> 0.5 </ind><dep>  45.0 </dep></entry>
      <entry><ind> 0.6 </ind><dep>  86.5 </dep></entry>
      <entry><ind> 0.7 </ind><dep> 135.0 </dep></entry>
      <entry><ind> 0.8 </ind><dep> 194.5 </dep></entry>
      <entry><ind> 0.9 </ind><dep> 257.5 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 321.5 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

