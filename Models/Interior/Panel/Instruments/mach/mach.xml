<?xml version="1.0"?>

<!-- Mach meter-->
<PropertyList>

	<path>mach.ac</path>

	<animation>
		<type>rotate</type>
		<object-name>mach-Needle1</object-name>
		<property>velocities/mach</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>45</dep>
			</entry>
			<entry>
				<ind>0.6</ind>
				<dep>86.5</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>135</dep>
			</entry>
			<entry>
				<ind>0.8</ind>
				<dep>194.5</dep>
			</entry>
			<entry>
				<ind>0.9</ind>
				<dep>257.5</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>321.5</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>material</type>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>
	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>
</PropertyList>
