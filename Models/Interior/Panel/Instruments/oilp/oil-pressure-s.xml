<?xml version="1.0"?>
  <PropertyList> 
    <path>oil-pressure.ac</path>
    
    <animation>
  <type>scale</type>
  <x-offset>0.70</x-offset>
  <y-offset>0.70</y-offset>
  <z-offset>0.70</z-offset>
 </animation>
 
    <animation> 
      <type>rotate</type>
      <object-name>Oil-Pressure-Needle1</object-name>
      <property>engines/engine[1]/n1</property>
      <interpolation> 
        <entry> 
          <ind>0</ind>
          <dep>-25</dep>
        </entry>
        <entry> 
          <ind>20</ind>
          <dep>-20</dep>
        </entry>
        <entry> 
          <ind>30</ind>
          <dep>-15</dep>
        </entry>
        <entry> 
          <ind>40</ind>
          <dep>-10</dep>
        </entry>
        <entry> 
          <ind>50</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>60</ind>
          <dep>5</dep>
        </entry>
        <entry> 
          <ind>70</ind>
          <dep>10</dep>
        </entry>
        <entry> 
          <ind>80</ind>
          <dep>10</dep>
        </entry>
		<entry> 
          <ind>90</ind>
          <dep>10</dep>
        </entry>
		<entry> 
          <ind>100</ind>
          <dep>10</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>-0.04</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
      <shininess>10</shininess>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
