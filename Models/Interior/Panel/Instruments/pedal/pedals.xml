<?xml version="1.0"?>
<PropertyList>

 <path>dc3-pedals.ac</path>

<animation>
  <type>rotate</type>
  <object-name>LeftPedal</object-name>
  <property>/controls/flight/rudder</property>
  <factor>25</factor>
  <center>
   <x-m>-0.012</x-m>
   <y-m>-0.000</y-m>
   <z-m>-0.100</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>RightPedal</object-name>
  <property>/controls/flight/rudder</property>
  <factor>-25</factor>
  <center>
   <x-m>-0.012</x-m>
   <y-m>-0.000</y-m>
   <z-m>-0.100</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
  
  
  
